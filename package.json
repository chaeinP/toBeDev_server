{
  "name": "server",
  "version": "1.0.0",
  "description": "toBeDev Project Server",
  "main": "index.js",
  "scripts": {
    "swagger": "tsoa swagger && mkdir -p dist && cp ./src/swagger.json ./dist/swagger.json",
    "routes": "tsoa routes",
    "build": "npm run routes && npm run swagger && tsc --build",
    "dev": "env-cmd -f .env.dev ts-node-dev -r tsconfig-paths/register --inspect --ignore-watch node_modules src/server.ts",
    "serve": "pm2 start dist/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.39",
    "mysql2": "^2.3.3",
    "swagger-ui-express": "^4.6.0",
    "tsoa": "^4.1.3",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
